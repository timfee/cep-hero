{
  "name": "cepts",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "test": "bun test",
    "evals:run": "bun test tests/evals/*.test.ts",
    "evals:run:pattern": "bun test tests/evals/*.test.ts --test-name-pattern \"$TEST_PATTERN\"",
    "evals:run:by-id": "EVAL_IDS=$EVAL_IDS bun test tests/evals/*.test.ts",
    "evals:run:by-tag": "EVAL_TAGS=$EVAL_TAGS bun test tests/evals/*.test.ts",
    "evals:run:plan": "EVAL_CATEGORY=test_plan bun test tests/evals/test-plan.test.ts",
    "evals:run:diag": "EVAL_CATEGORY=diagnostics bun test tests/evals/diagnostics.test.ts",
    "evals:run:common": "EVAL_CATEGORY=common_challenges bun test tests/evals/common-challenges.test.ts",
    "evals:run:fast": "EVAL_MANAGE_SERVER=0 bun test tests/evals/*.test.ts",
    "evals:run:plan:fast": "EVAL_MANAGE_SERVER=0 EVAL_CATEGORY=test_plan bun test tests/evals/test-plan.test.ts",
    "evals:run:diag:fast": "EVAL_MANAGE_SERVER=0 EVAL_CATEGORY=diagnostics bun test tests/evals/diagnostics.test.ts",
    "evals:run:common:fast": "EVAL_MANAGE_SERVER=0 EVAL_CATEGORY=common_challenges bun test tests/evals/common-challenges.test.ts",
    "fixtures:capture": "bun run scripts/capture-eval-fixtures.ts",
    "credentials:check": "bun run scripts/check-credentials.ts",
    "check": "ultracite check",
    "fix": "ultracite fix"
  },
  "dependencies": {
    "@ai-sdk/google": "^3.0.18",
    "@ai-sdk/react": "^3.0.64",
    "@modelcontextprotocol/sdk": "^1.25.3",
    "@upstash/vector": "^1.2.0",
    "ai": "^6.0.62",
    "better-auth": "^1.4.18",
    "clsx": "^2.1.1",
    "crawlee": "^3.15.3",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.7",
    "framer-motion": "^12.29.2",
    "google-auth-library": "^10.5.0",
    "googleapis": "^170.1.0",
    "lucide-react": "^0.563.0",
    "next": "16.1.6",
    "react": "19.2.4",
    "react-dom": "19.2.4",
    "react-markdown": "^10.1.0",
    "remark-gfm": "^4.0.1",
    "tailwind-merge": "^3.4.0",
    "turndown": "^7.2.1",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.18",
    "@types/bun": "^1.2.0",
    "@types/node": "^25.1.0",
    "@types/open": "^6.2.1",
    "@types/react": "^19.2.10",
    "@types/react-dom": "^19.2.3",
    "@types/turndown": "^5.0.5",
    "babel-plugin-react-compiler": "1.0.0",
    "open": "^11.0.0",
    "oxfmt": "^0.27.0",
    "oxlint": "^1.42.0",
    "oxlint-tsgolint": "^0.11.3",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3",
    "ultracite": "7.1.1"
  },
  "ignoreScripts": [
    "sharp",
    "unrs-resolver"
  ],
  "trustedDependencies": [
    "sharp",
    "unrs-resolver"
  ]
